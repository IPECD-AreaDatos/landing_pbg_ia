# Archivo de ejemplo para variables de entorno en Vercel
# Copia estas variables en tu dashboard de Vercel

APP_NAME="PBG Corrientes Dashboard"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://tu-dominio.vercel.app

LOG_CHANNEL=stderr
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# Base de datos - Reemplaza con tus credenciales reales
DB_CONNECTION=mysql
DB_HOST=tu-host-mysql
DB_PORT=3306
DB_DATABASE=datalake_economico
DB_USERNAME=tu-usuario
DB_PASSWORD=tu-contrase√±a

# Cache drivers optimizados para Vercel
BROADCAST_CONNECTION=log
CACHE_STORE=array
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=cookie
SESSION_LIFETIME=120

# Otros
VITE_APP_NAME="${APP_NAME}"